<h1>Listing High Scores</h1>

<table>
	<thead>
		<tr>
			<th>User</th>
			<th>Game</th>
			<th>Score</th>
			<th colspan="3"></th>
		</tr>
	</thead>
	<tbody>
		<% @high_scores.each do |hs| %>
			<tr>
				<td><%= hs.user %></td>
				<td><%= hs.game %></td>
				<td><%= hs.score %></td>
				<td><%= link_to "Show", hs %></td>
				<td><%= link_to "Edit", edit_high_score_path(hs) %></td>
			</tr>
		<% end %>
	</tbody>
</table>

<%= link_to "Add new High Score", new_high_score_path %>